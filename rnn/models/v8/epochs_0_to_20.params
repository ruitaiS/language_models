{
    "vocab_size": 70,
    "tokenization": "char",
    "include_book": true,
    "embedding_dim": 32,
    "hidden_dim": 512,
    "lstm_layers": 3,
    "embedding_dropout": 0.3,
    "lstm_dropout": 0.5,
    "fc_dropout": 0.5,
    "lr": 0.001,
    "batch_size": 50,
    "seq_len": 100,
    "validation_p": 0.1,
    "shuffle": true,
    "reset_each": "batch",
    "clip_grad": 5,
    "epochs": 20,
    "use_gpu": false
}